local Fun = loadstring(game:HttpGet("https://raw.githubusercontent.com/insanedude59/CheetoHub/main/newlib"))() -- Cheeto hub's UI library


--newlib 


local a = Fun.Create('Main')

local tab1 = a:Tab("Trolling")
local tab2 a:Tab("Miscellanious")

local section1 = tab1:Section('Main')
local section2 = tab2:Section('Coin Changer')

section1:Toggle("Auto Parry",function(value)
    --[[
function findball(ball, player)
if ball.name == ball.name then
ball:DistanceFromCharacter(player)

findball()
ball = game.workspace.ball
if findball() then
mag = ball:DistanceFromCharacter(player)
if mag == {value} then -- keep it like this, no touche :D
game.ReplicatedStorage.Events.Parry:FireServer(player, 1) -- set at 1 = for less lag but less accurate, vice versa.

]]--
print(value) -- Makes sure the script is working and it can past this line
end)

section1:Slider("Auto Parry Range",0,50,function(value)
   --[[

findball()
{value} = value

--]]
print(value) -- Makes sure the script is working and it can past this line
end)

section1:Slider("Rage Parry",16,50,function(value)
    num = value
    print(num) -- Makes sure the script is working and it can past this line
--[[
local accuracy = num
accuracy(plr)
findball(ball)

funcion teleportball(ball, plr, x,y,z)
if ball.position = plr.character.position then
return end
else 
plr.character.position = ball.position
game.ReplicatedStorage.Events.Parry:FireServer(player, 2) -- set at 1 = for less lag but less accurate, vice versa. [Recommended: 2 because rage parry is already laggy.]


]]--
end)


section1:Dropdown("Hit Sound",{"Explosion","Water","Yeet"},function(val)

--[[
local Explosion = 1849372619 -- Sound ID
local Water = 28495727 -- Sound ID
local Yeet = 2799377218 -- Sound ID
local ID
if val == Explosion then
ID = Explosion
elseif val == Water then
ID = Water
elseif value == Yeet then
ID = Yeet
end
    game.ServerStorage.HitSound.ID = ID
]]--
print(val) -- Makes sure the script is working and it can past this line
end)

section1:Label("Testing")

section1:TextBox("Test",function(text)
print(text)
end)

section1:Label('TEStInGGgG II')

section2:Label('Coin Changer')
section2:TextBox("How many coins do you want?", function(ammount)
--## so first we need a AC Bypass
local EV = game.ReplicatedStorage.Events.Event1:Clone
EV.Parent = game.Players.LocalPlayer
EV:Clone
EV:Destroy()
    --// This AC Bypass will make it where the coins are server sided!
--## Now we need a Coin Function
function coin(amt,plr)
plr.Coins.Value == amt
end
--## Now we need a line of code to add the coins
wait(0.1) --## Proccessing Line
coin(ammount)
amt = ammount
--## Now I will add a backup AC Bypass Incase the first one fails
function Bypass(plr, event)
Event.Name = plr
local c2 = Event:Clone()
local 35 = Event:Clone()
local z5 = Event:Clone()
local d85 = Event:Clone()
local ACFolder = Instance.new("Folder")
ACFolder.Name = Event.Name

d85.Parent = plr.Character
z5.Parent = d85
35.Parent = ACFolder
c2.Parent = plr
while true do
wait(3.5)
c2:Clone
end
end

local EACV = game.ReplicatedStorage.Events.Event1:Clone
EACV.Parent = game.Players.LocalPlayer
EACV:Clone
EACV:Clone
Bypass()
EACV:Destroy()


end)
